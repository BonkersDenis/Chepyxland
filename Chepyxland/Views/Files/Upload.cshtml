@{
    ViewData["Title"] = "Upload File";
}

<h2>Upload File</h2>

<form asp-action="Upload" method="post" enctype="multipart/form-data">
    <div>
        <label for="file">Choose file:</label>
        <input type="file" name="file" id="file" required />
    </div>
    <button type="submit">Upload</button>
</form>